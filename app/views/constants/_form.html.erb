<%= form_with(model: constant, local: true) do |form| %>
  <% if constant.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(constant.errors.count, "error") %> prohibited this constant from being saved:</h2>

      <ul>
      <% constant.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :key %>
    <%= form.text_field :key, id: :constant_key %>
  </div>

  <div class="field">
    <%= form.label :value %>
    <%= form.text_field :value, id: :constant_value %>
  </div>

  <div class="field">
    <%= form.label :created %>
    <%= form.datetime_select :created, id: :constant_created %>
  </div>

  <div class="field">
    <%= form.label :updated %>
    <%= form.datetime_select :updated, id: :constant_updated %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
